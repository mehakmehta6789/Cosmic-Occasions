  <!-- Custom CSS -->
  <link rel="stylesheet" href="contact.css" />

  <title>Contact</title>
</head>
<body>

  <!-- NAVBAR SECTION -->
  <nav class="navbar navbar-expand-sm sticky-top navbar-dark" style="background-color:#6c7d5a">
    <div class="container-fluid">
